{
  "name": "streamwave-host",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "rollup -wc build/rollup.config.js",
    "server": "nodemon --inspect server/index.js",
    "build": "rm -rf ./dist && rollup -c build/rollup.config.production.js && node build/cache-manifest-builder.js",
    "serve": "NODE_ENV=production node server/index.js",
    "bundlesize": "bundlesize",
    "postinstall": "mkdirp /tmp/uploads"
  },
  "bundlesize": [
    {
      "path": "./dist/*.bundle.*.js",
      "maxSize": "200kB"
    }
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "babel-loader": "^8.0.0-beta.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-imports": "^1.5.0",
    "bundlesize": "^0.17.0",
    "css-loader": "^0.28.11",
    "dotenv": "^5.0.1",
    "fast-async": "^6.3.7",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "sass-loader": "^6.0.7",
    "walk": "^2.3.13"
