    "idb-keyval": "^3.0.5",
    "material-ui": "^1.0.0-beta.47",
    "mime": "^2.3.1",
    "mongoose": "^5.1.5",
    "multer": "^1.3.0",
    "music-metadata": "^2.0.4",
    "nodemailer": "^4.6.7",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "redis": "^2.8.0",
