<div class="mini-player">
  <section class="mini-player__progress-container">
    {>"components/progress-bar-presentation" /}
  </section>

  <section class="mini-player__cover-container">
    <img class="mini-player__cover-artwork" alt="mini cover artwork"/>
  </section>

  <section class="mini-player__infos-container">
    <div class="mini-player__artist"></div>
    <div class="mini-player__title"></div>
  </section>

  <section class="mini-player__controls-container">
